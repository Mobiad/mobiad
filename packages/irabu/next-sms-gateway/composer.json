{
  "name": "irabu/next-sms-gateway",
  "description": "A nextsms gateway",
  "license": "MIT",
  "keywords": [
    "next",
    "sms",
    "gateway"
  ],
  "type": "library",
  "authors": [
    {
      "name": "",
      "email": ""
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/support": "^6.0"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Irabu\\NextSmsGateway\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Irabu\\NextSmsGateway\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Irabu\\NextSmsGateway\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
